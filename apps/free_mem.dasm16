:free
JSR [0x1011] ; page_check
SET X, 0xFFFF
SUB X, A
SET A, X
SET B, free_buffer
SET C, 10
JSR [0x101B] ; itoa
SET B, free_buffer2
DIV A, 1024
SHL A, 1
SET C, 10
JSR [0x101B] ; itoa
SET A, free_buffer
JSR [0x101E] ; text_out
SET A, free_buffer
JSR [0x1005] ; proc_kill_me
:free_buffer dat "      words free ("
:free_buffer2 dat "      KB)", 0xA0, 0x00
:free_end
