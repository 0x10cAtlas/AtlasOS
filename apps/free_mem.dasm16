:free
JSR [0x1011] ; page_check
SET X, 0xFFFF
SUB X, A
SET A, X
SET B, free_buffer
SET C, 10
JSR [0x101B] ; itoa
SET A, X
SHR A, 9
SET B, free_buffer2
SET C, 10
JSR [0x101B] ; itoa
SET A, free_buffer
:free_loop
IFE [A], 0
	SET [A], 32
ADD A, 1
IFN [A], 0x0a
	SET PC, free_loop
SET A, free_buffer
JSR [0x101E] ; text_out
JSR [0x1005] ; proc_kill_me
:free_buffer dat "      words free ("
:free_buffer2 dat "      KB)", 0x0a, 0x00
:free_end
